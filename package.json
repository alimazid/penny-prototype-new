{
  "name": "penny-prototype",
  "version": "1.0.0",
  "description": "Email Processing Pipeline Prototype for Penny AI Financial Monitor",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "jest",
    "test:integration": "tsx test/integration.test.ts",
    "test:pipeline": "tsx scripts/test-pipeline.ts",
    "db:migrate": "npx prisma migrate dev",
    "db:generate": "npx prisma generate",
    "db:studio": "npx prisma studio",
    "setup": "chmod +x scripts/setup.sh && ./scripts/setup.sh",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "dotenv": "^16.3.1",
    "googleapis": "^129.0.0",
    "openai": "^4.20.1",
    "bullmq": "^4.15.1",
    "redis": "^4.6.10",
    "socket.io": "^4.7.4",
    "prisma": "^5.7.0",
    "@prisma/client": "^5.7.0",
    "zod": "^3.22.4",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "multer": "^1.4.5-lts.1",
    "winston": "^3.11.0",
    "node-cron": "^3.0.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/bcryptjs": "^2.4.6",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.0",
    "@types/node-cron": "^3.0.11",
    "typescript": "^5.3.2",
    "tsx": "^4.6.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.8",
    "ts-jest": "^29.1.1",
    "eslint": "^8.55.0",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "prettier": "^3.1.0",
    "nodemon": "^3.0.2"
  },
  "keywords": [
    "email",
    "ai",
    "financial",
    "gmail",
    "processing",
    "pipeline",
    "prototype"
  ],
  "author": "Penny Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  }
}